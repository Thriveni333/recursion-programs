class Employee{
    int id;
    String name;
    String roll;
    double salary;
    static String company="Wipro";
    Employee(int id,String name,String roll,double salary){
        this.id=id;
        this.name=name;
        this.roll=roll;
        this.salary=salary;
        this.company=company;
    }
    public void display(){
        System.out.println("Name is:"+this.name);
        System.out.println("id is:"+this.id);
        System.out.println("roll is:"+this.roll);
        System.out.println("salary is:"+this.salary);
        System.out.println("company is:"+Employee.company);
        System.out.println();
    }
}
class EmpData{
    public static void main(String[] args){
        Employee e1=new Employee(101,"Umer","Python FS",59000);
        Employee e2=new Employee(102,"Veni","manager",10000);
        e1.display();
        e2.display();
        
    }
}

        
